<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<GridPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="View.GUI.ControllerMainWindow" prefHeight="600.0" prefWidth="1000.0" vgap="10.0" hgap="10.0">
    <padding>
        <Insets bottom="10.0" left="25.0" right="25.0" top="25.0" />
    </padding>
    <TableView fx:id="heap_table_view" accessibleRole="IMAGE_VIEW" editable="true" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="0" GridPane.columnIndex="0">
        <columns>
            <TableColumn fx:id="heap_address_column" prefWidth="100.0" text="Address"/>
            <TableColumn fx:id="heap_value_column" prefWidth="100.0" text="Value"/>
        </columns>
    </TableView>
    <ListView fx:id="out_list_view" editable="true" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="0" GridPane.columnIndex="1"/>
    <ListView fx:id="file_list_view" editable="true" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="0" GridPane.columnIndex="2"/>
    <ListView fx:id="programs_list_view" editable="true" prefHeight="400.0" prefWidth="200.0" GridPane.rowIndex="0" GridPane.columnIndex="3"/>
    <Button mnemonicParsing="false" onAction="#handleRunOneStepButton" prefHeight="30" prefWidth="90" text="Run One Step" GridPane.rowIndex="1" GridPane.columnIndex="5"/>
</GridPane>
